{
  "chrome": {
    "default": "latest",
    "versions": {
      "latest": {
        "image": "selenoid/vnc:chrome_latest",
        "port": "4444",
        "path": "/",
        "tmpfs": {"/tmp": "size=512m"},
        "env": ["ENABLE_VNC=true", "SCREEN_WIDTH=1920", "SCREEN_HEIGHT=1080", "SCREEN_DEPTH=24"],
        "args": ["--no-sandbox", "--disable-dev-shm-usage", "--disable-gpu", "--headless"]
      }
    }
  }
}